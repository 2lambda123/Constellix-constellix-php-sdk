{
    "name": "tiggee/constellix-v4-client",
    "description": "PHP SDK for the Constellix API",
    "type": "library",
    "require": {
        "ext-json": "^7.4||^8.0",
        "php": "^7.4||^8.0",
        "ext-mbstring": "^7.4||^8.0",
        "psr/container": "^1.0||^2.0",
        "psr/http-client-implementation": "^1.0||^2.0",
        "guzzlehttp/psr7": "^1.7||^2.0",
        "psr/log": "^2.0||^3.0",
        "spatie/enum": "^3.6",
        "guzzlehttp/guzzle": ">=6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-http/mock-client": "^1.4",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "monolog/monolog": "^2.1",
        "symfony/var-dumper": "^5.1",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Constellix\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConstellixTest\\": "test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Smith",
            "email": "jessica.smith@digicert.com"
        }
    ],
    "minimum-stability": "stable"
}
