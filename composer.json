{
    "name": "tiggee/constellix-v4-client",
    "description": "PHP SDK for the Constellix API",
    "type": "library",
    "require": {
        "ext-json": "^8.0||^8.1||^8.2||^8.3",
        "php": "^8.0||^8.1||^8.2||^8.3",
        "ext-mbstring": "^8.0||^8.1||^8.2||^8.3",
        "spatie/enum": "^3.13",
        "psr/log": "^3.0",
        "guzzlehttp/psr7": "^2.6",
        "nesbot/carbon": "^2.69"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Constellix\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Constellix\\Client\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "phpstan": "vendor/bin/phpstan",
        "phpunit": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Smith",
            "email": "jessica.smith@digicert.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.22",
        "php-http/mock-client": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "monolog/monolog": "^2",
        "symfony/var-dumper": "^6.3",
        "http-interop/http-factory-guzzle": "^1.2",
        "guzzlehttp/guzzle": "^7.7",
        "kylekatarnls/carbonite": "^1.4"
    }
}
